{
  "short_name": "GetVerified",
  "name": "Auth News",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon-not-sure.png",
    "default_popup": "index.html",
    "default_title": "Get Verified"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["/main.css"],
      "js": ["/main.js"]
    }
  ]
}
